! Topologie konfigurieren
! R01 = ISP     g0/1 = 192.186.10.2  255.255.255.252
!               g0/2 = 192.168.10.6  255.255.255.252
!               g0/0 = 192.186.10.10 255.255.255.252
!
! R02:  g0/0 = 192.168.0.254 255.255.255.0
!       g0/1 = 192.168.10.1  255.255.255.252
!
! R03:  g0/0 = 192.168.0.253 255.255.255.0
!       g0/1 = 192.186.10.5  255.255.255.252
!
! R04   g0/0 = 192.168.0.252 255.255.255.0
!       g0/1 = 192.186.10.9  255.255.255.252
!
! ISP-Router configuration
enable
    configure terminal
    interface g0/0
        ip address 192.186.10.10 255.255.255.252
        no shutdown
    interface g0/1
        ip address 192.186.10.2 255.255.255.252
        no shutdown
    interface g0/2
        ip address 192.186.10.6 255.255.255.252
        no shutdown
    end
write
!
! R02 configuration
!
enable
    configure terminal
    hostname R02
    interface loopback 0
        ip address 1.1.1.1 255.255.255.0
    router eigrp 1
        network 1.1.1.0 0.0.0.255
        network 192.186.10.0 0.0.0.3
        network 192.168.0.0 0.0.0.255
        passive-interface g0/2
        passive-interface g0/1
    interface g0/0
        standby version 2
    interface g0/0.1
        encapsulation dot1q 1
        ip address 192.168.0.254 255.255.255.0
        standby 1 priority 115
        standby 1 preempt 
        standby 1 ip 192.168.0.250
    interface g0/0
        standby version 2
        no shutdown
    interface g0/1
        ip address 192.186.10.1 255.255.255.252
        no shutdown
    end 
write
!
! R03 configuration
!
enable
    configure terminal 
    hostname R03
    interface loopback 0
        ip address 1.1.1.2 255.255.255.0
    router eigrp 1
        network 1.1.1.0 0.0.0.255
        network 192.186.10.0 0.0.0.3
        network 192.168.0.0 0.0.0.255
        passive-interface g0/1
        passive-interface g0/2
    interface g0/0
        standby version 2
    interface g0/0.1
        encapsulation dot1q 1
        ip address 192.168.0.253 255.255.255.0
        standby 1 priority 100
        standby 1 ip 192.168.0.250
    interface g0/0
        no shutdown
    interface g0/2
        ip address 192.186.10.5 255.255.255.252
        no shutdown
    end
write 
!
! R04 configuration
!
enable
    configure terminal
    hostname R04
    interface loopback 0
        ip address 1.1.1.3 255.255.255.0
    router eigrp 1
        network 1.1.1.0 0.0.0.255
        network 192.186.10.0 0.0.0.3
        network 192.168.0.0 0.0.0.255
        passive-interface g0/1
        passive-interface g0/2
    interface g0/0
        standby version 2
    interface g0/0.1
        encapsulation dot1q 1
        ip address 192.168.0.252 255.255.255.0
        standby 1 priority 105
        standby 1 ip 192.168.0.250 
    interface g0/0
        no shutdown
    interface g0/1
        ip address 192.186.10.09 255.255.255.252
        no shutdown
    end
write
!
! SW1 configuration
!
enable
    configure terminal 
    hostname SW1
    interface vlan 1
        no shutdown
    interface g0/1
        switchport mode access
        switchport access vlan 1
    interface g0/2
        switchport mode access
        switchport access vlan 1
    interface fa0/5
        switchport mode access
        switchport access vlan 1
        spanning-tree portfast
    end
write
!
! SW02 configuration
!
enable
    configure terminal
    hostname SW02
    interface vlan 1
        no shutdown
    interface g0/1
        switchport mode access
        switchport access vlan 1
    interface g0/2
        switchport mode access
        switchport access vlan 1
    interface range f0/1-4
        channel-group 1 mode active
    interface port-channel 1
        switchport mode trunk
        switchport trunk native vlan 1
        switchport trunk allowed vlan 1
        switchport nonegotiate
    interface f0/5
        switchport mode access
        switchport access vlan 1
        spanning-tree portfast 
    end
write
!
! SW03 configuration
!
enable
    configure terminal
        interface vlan 1
            no shutdown
        interface g0/1
            switchport mode access
            switchport access vlan 1
        interface range f0/1-4
            channel-group 1 mode passive
        interface port-channel 1
            switchport mode trunk
            switchport trunk native vlan 1
            switchport trunk allowed vlan 1
            switchport nonegotiate
        interface f0/5
            switchport mode access
            switchport access vlan 1
            spanning-tree portfast
    end
write
